<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd%22%3E">
<hibernate-mapping package="nanterre.miage.baptiste.model">
	<class name="Group" table="groupe">
	<id name="idGroup" type="int" column="idGroup" >
		<generator class="increment"/>
	</id>
	<version name="version" access="field" column="VERSION" />
	
	<property name="groupName">
		<column name="groupName" />
	</property>
	
	<set name="contacts" table="contact_group" >
		<key column="idGroup"></key>
		<many-to-many class="Contact" column="idContact"/>
	</set>
	
  </class>
</hibernate-mapping>